  <!-- Jibartik Start: This is a static aniamtion of a classic background -->

  <Ui2DAnimation item="Jib_CharSelectBackgroundTexture">
    <Cycle>true</Cycle>
    <Frames>
      <Texture>classic_backgrounds01.tga</Texture>
      <Location>
        <X>0</X>
        <!-- changes texture postion -->
        <Y>0</Y>
        <!-- changes texture postion -->
      </Location>
      <Size>
        <CX>712</CX>
        <!-- changes size of image -->
        <CY>960</CY>
        <!-- changes size of image -->
      </Size>
      <Duration>1000</Duration>
    </Frames>
  </Ui2DAnimation>

  <!-- this is the end of the pressed button -->

  <!-- Jibartik Start: This positions a static aniamtion on the ui -->

  <StaticAnimation item="Jib_CharSelectBackgroud">
    <RelativePosition>true</RelativePosition>
    <Location>
      <X>0</X>
      <!-- will move the image around -->
      <Y>0</Y>
      <!-- will move the image around -->
    </Location>
    <Size>
      <CX>712</CX>
      <!-- stretches button around -->
      <CY>960</CY>
      <!-- stretches button around -->
    </Size>
    <Animation>Jib_CharSelectBackgroundTexture</Animation>
  </StaticAnimation>

