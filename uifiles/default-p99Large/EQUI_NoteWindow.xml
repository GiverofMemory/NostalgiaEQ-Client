<?xml version="1.0" encoding="us-ascii"?>
<XML ID="EQInterfaceDefinitionLanguage">
  <Schema xmlns="EverQuestData" xmlns:dt="EverQuestDataTypes" />

  <STMLbox item="NW_NoteDisplay">
    <ScreenID>Note</ScreenID>
    <DrawTemplate>WDT_Inner</DrawTemplate>
    <RelativePosition>true</RelativePosition>
    <Style_VScroll>false</Style_VScroll>
    <Style_Border>false</Style_Border>
    <Style_Transparent>true</Style_Transparent>
    <Font>6</Font>
    <TextColor>
      <R>0</R>
      <G>0</G>
      <B>0</B>
    </TextColor>
    <Location>
      <X>150</X>
      <Y>150</Y>
    </Location>
    <Size>
      <CX>500</CX>
      <CY>400</CY>
    </Size>
    <Text>Test text</Text>
  </STMLbox>

  <StaticAnimation item="NW_BG">
    <ScreenID>BG1</ScreenID>
    <RelativePosition>true</RelativePosition>
    <Location>
      <X>0</X>
      <Y>0</Y>
    </Location>
    <Size>
      <CX>800</CX>
      <CY>640</CY>
    </Size>
    <Animation>A_NoteBG</Animation>
  </StaticAnimation>


  <Button item="NW_DoneButton">
    <ScreenID>DoneButton</ScreenID>
    <Font>5</Font>
    <RelativePosition>true</RelativePosition>
    <Location>
      <X>352</X>
      <Y>558</Y>
    </Location>
    <Size>
      <CX>96</CX>
      <CY>38</CY>
    </Size>
    <Style_VScroll>false</Style_VScroll>
    <Style_HScroll>false</Style_HScroll>
    <Style_Transparent>false</Style_Transparent>
    <!--<TooltipReference/>-->
    <Style_Checkbox>false</Style_Checkbox>
    <!--<RadioGroup/>-->
    <Text>DONE</Text>
    <TextColor>
      <R>51</R>
      <G>51</G>
      <B>51</B>
    </TextColor>
    <ButtonDrawTemplate>
      <Normal>A_BtnNormal</Normal>
      <Pressed>A_BtnPressed</Pressed>
      <Flyby>A_BtnFlyby</Flyby>
      <Disabled>A_BtnDisabled</Disabled>
      <PressedFlyby>A_BtnPressedFlyby</PressedFlyby>
    </ButtonDrawTemplate>
    <!--<SoundPressed/>-->
    <!--<<SoundUp/>-->
    <!--<SoundFlyby/>-->
  </Button>

  <Screen item="NoteWindow">
    <!--<ScreenID/>-->
    <Font>4</Font>
    <RelativePosition>false</RelativePosition>
    <Location>
      <X>100</X>
      <Y>80</Y>
    </Location>
    <Size>
      <CX>800</CX>
      <CY>640</CY>
    </Size>
    <Style_VScroll>false</Style_VScroll>
    <Style_HScroll>false</Style_HScroll>
    <Style_Transparent>false</Style_Transparent>
    <TooltipReference>This is an Item Display window</TooltipReference>
    <DrawTemplate>WDT_RoundedNoTitle</DrawTemplate>
    <Style_Titlebar>false</Style_Titlebar>
    <Style_Closebox>false</Style_Closebox>
    <Style_Minimizebox>false</Style_Minimizebox>
    <Style_Border>false</Style_Border>
    <Style_Sizable>false</Style_Sizable>
    <Text>A Note</Text>
    <Pieces>NW_BG</Pieces>
    <Pieces>NW_NoteDisplay</Pieces>
    <Pieces>NW_DoneButton</Pieces>
  </Screen>
</XML>