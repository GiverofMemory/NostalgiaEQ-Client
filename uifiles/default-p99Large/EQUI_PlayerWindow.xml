<?xml version="1.0" encoding="us-ascii"?>
<XML ID="EQInterfaceDefinitionLanguage">
  <Schema xmlns="EverQuestData" xmlns:dt="EverQuestDataTypes" />

  <StaticAnimation item="PWDummy">
    <ScreenID>PWDummy</ScreenID>
    <Location>
      <X>-2</X>
      <Y>-2</Y>
    </Location>
    <Size>
      <CX>16</CX>
      <CY>16</CY>
    </Size>
    <Animation>A_CornerSquare</Animation>
  </StaticAnimation>

<!-- Attack Indicator -->

  <TextureInfo item="AttackIndicator.tga">
    <Size>
      <CX>512</CX>
      <CY>512</CY>
    </Size>
  </TextureInfo>

  <Ui2DAnimation item="A_AttackIndicator">
    <Cycle>false</Cycle>
    <Frames>
      <Texture>AttackIndicator.tga</Texture>
      <Location>
        <X>0</X>
        <Y>0</Y>
      </Location>
    <Size>
      <CX>288</CX>
      <CY>210</CY>
    </Size>
    </Frames>
  </Ui2DAnimation>

  <StaticAnimation item="A_AttackIndicatorAnim">
    <ScreenID>A_AttackIndicatorAnim</ScreenID>
    <Animation>A_AttackIndicator</Animation>
  </StaticAnimation>


<!-- GAUGES -->

  <Gauge item="Player_HP">
    <ScreenID>PlayerHP</ScreenID>
    <Font>1</Font>
    <TextColor>
      <R>240</R>
      <G>240</G>
      <B>240</B>
    </TextColor>
    <RelativePosition>true</RelativePosition>
    <Location>
      <X>30</X>
      <Y>0</Y>
    </Location>
    <Size>
      <CX>200</CX>
      <CY>120</CY>
    </Size>
    <TextOffsetX>-80</TextOffsetX>
    <TextOffsetY>0</TextOffsetY>
    <GaugeOffsetY>80</GaugeOffsetY>
    <GaugeOffsetX>0</GaugeOffsetX>
    <Style_VScroll>false</Style_VScroll>
    <Style_HScroll>false</Style_HScroll>
    <Style_Transparent>true</Style_Transparent>
    <TooltipReference />
    <FillTint>
		<Alpha>255</Alpha>
      <R>255</R>
      <G>255</G>
      <B>255</B>
    </FillTint>
    <LinesFillTint>
      <R>0</R>
      <G>0</G>
      <B>0</B>
    </LinesFillTint>
    <DrawLinesFill>true</DrawLinesFill>
    <EQType>1</EQType>
    <GaugeDrawTemplate>
      <Background>A_GaugeBackground</Background>
      <Fill>A_RedGaugeFill</Fill>
    </GaugeDrawTemplate>
  </Gauge>


  <Label item="Player_NameLabel">
