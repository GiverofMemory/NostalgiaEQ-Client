<?xml version="1.0" encoding="us-ascii"?>
<XML ID="EQInterfaceDefinitionLanguage">
  <Schema xmlns="EverQuestData" xmlns:dt="EverQuestDataTypes" />

  <Listbox item="SKLW_SkillList">
    <Font>5</Font>
    <ScreenID>SkillList</ScreenID>
    <DrawTemplate>WDT_Inner</DrawTemplate>
    <AutoStretch>true</AutoStretch>
    <RightAnchorToLeft>false</RightAnchorToLeft>
    <BottomAnchorToTop>false</BottomAnchorToTop>
    <LeftAnchorOffset>4</LeftAnchorOffset>
    <TopAnchorOffset>4</TopAnchorOffset>
    <RightAnchorOffset>4</RightAnchorOffset>
    <BottomAnchorOffset>48</BottomAnchorOffset>
    <Style_Border>true</Style_Border>
    <Style_VScroll>true</Style_VScroll>
    <Columns>
      <Width>160</Width>
      <Heading>Skill Name</Heading>
    </Columns>
    <Columns>
      <Width>120</Width>
      <Heading>Rank</Heading>
    </Columns>
    <Columns>
      <Width>60</Width>
      <Heading>Value</Heading>
    </Columns>
  </Listbox>

  <Button item="SKLW_DoneButton">
    <ScreenID>DoneButton</ScreenID>
    <Font>5</Font>
    <RelativePosition>true</RelativePosition>
    <AutoStretch>true</AutoStretch>
    <LeftAnchorOffset>144</LeftAnchorOffset>
    <TopAnchorOffset>48</TopAnchorOffset>
    <RightAnchorOffset>4</RightAnchorOffset>
    <BottomAnchorOffset>4</BottomAnchorOffset>
    <TopAnchorToTop>false</TopAnchorToTop>
    <BottomAnchorToTop>false</BottomAnchorToTop>
    <RightAnchorToLeft>false</RightAnchorToLeft>
    <LeftAnchorToLeft>false</LeftAnchorToLeft>
    <Style_Transparent>false</Style_Transparent>
    <!--<TooltipReference/>-->
    <Style_Checkbox>false</Style_Checkbox>
    <!--<RadioGroup/>-->
    <Text>Done</Text>
    <TextColor>
      <R>51</R>
      <G>51</G>
      <B>51</B>
    </TextColor>
    <ButtonDrawTemplate>
      <Normal>A_BtnNormal</Normal>
      <Pressed>A_BtnPressed</Pressed>
      <Flyby>A_BtnFlyby</Flyby>
      <Disabled>A_BtnDisabled</Disabled>
      <PressedFlyby>A_BtnPressedFlyby</PressedFlyby>
    </ButtonDrawTemplate>
    <!--<SoundPressed/>-->
    <!--<<SoundUp/>-->
    <!--<SoundFlyby/>-->
  </Button>

  <Button item="SKLW_MakeHotKeyButton">
    <ScreenID>MakeHotKeyButton</ScreenID>
    <Font>5</Font>
    <RelativePosition>true</RelativePosition>
    <AutoStretch>true</AutoStretch>
    <LeftAnchorOffset>284</LeftAnchorOffset>
    <TopAnchorOffset>48</TopAnchorOffset>
    <RightAnchorOffset>148</RightAnchorOffset>
    <BottomAnchorOffset>4</BottomAnchorOffset>
    <TopAnchorToTop>false</TopAnchorToTop>
    <BottomAnchorToTop>false</BottomAnchorToTop>
    <RightAnchorToLeft>false</RightAnchorToLeft>
    <LeftAnchorToLeft>false</LeftAnchorToLeft>
    <Style_Transparent>false</Style_Transparent>
    <!--<TooltipReference/>-->
    <Style_Checkbox>false</Style_Checkbox>
    <!--<RadioGroup/>-->
    <Text>Hotkey</Text>
    <TextColor>
      <R>51</R>
      <G>51</G>
      <B>51</B>
    </TextColor>
    <ButtonDrawTemplate>
      <Normal>A_BtnNormal</Normal>
      <Pressed>A_BtnPressed</Pressed>
      <Flyby>A_BtnFlyby</Flyby>
      <Disabled>A_BtnDisabled</Disabled>
      <PressedFlyby>A_BtnPressedFlyby</PressedFlyby>
    </ButtonDrawTemplate>
    <!--<SoundPressed/>-->
    <!--<<SoundUp/>-->
    <!--<SoundFlyby/>-->
  </Button>

  <Screen item="SkillsWindow">
    <!--<ScreenID/>-->
    <Font>6</Font>
    <RelativePosition>false</RelativePosition>
    <Location>
      <X>120</X>
      <Y>445</Y>
    </Location>
    <Size>
      <CX>400</CX>
      <CY>400</CY>
    </Size>
    <Style_VScroll>false</Style_VScroll>
    <Style_HScroll>false</Style_HScroll>
    <Style_Transparent>false</Style_Transparent>
    <TooltipReference />
    <Text>Skills</Text>
    <DrawTemplate>WDT_RoundedNoTitle2</DrawTemplate>
    <Style_Titlebar>true</Style_Titlebar>
    <Style_Closebox>true</Style_Closebox>
    <Style_Minimizebox>false</Style_Minimizebox>
    <Style_Border>true</Style_Border>
    <Style_Sizable>false</Style_Sizable>
    <Pieces>SKLW_SkillList</Pieces>
    <Pieces>SKLW_DoneButton</Pieces>
    <Pieces>SKLW_MakeHotKeyButton</Pieces>
  </Screen>
</XML>